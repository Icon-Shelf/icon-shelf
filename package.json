{
  "name": "icon-shelf",
  "version": "0.1.4",
  "productName": "Icon Shelf",
  "description": "Icon Shelf is a free icon manager for developers",
  "private": true,
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "node scripts/build.js",
    "precompile": "cross-env MODE=production npm run build",
    "compile": "electron-builder build --config .electron-builder.config.js",
    "publish": "npm run precompile && electron-builder build --config .electron-builder.config.js --publish always -mwl",
    "pretest": "npm run build",
    "test": "node tests/app.spec.js",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,ts,tsx",
    "typecheck-main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck-preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck-renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck-main && npm run typecheck-preload && npm run typecheck-renderer",
    "pretypecheck-renderer": "dts-cb -i packages/preload/src/**/*.ts -o packages/preload/exposedInMainWorld.d.ts"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "2.2.1",
    "@types/htmltojsx": "^0.0.30",
    "@types/lodash": "^4.14.172",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/svgo": "^2.6.0",
    "@types/universal-analytics": "^0.4.5",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@vitejs/plugin-react": "^1.1.3",
    "autoprefixer": "^10.4.1",
    "clean-terminal-webpack-plugin": "^3.0.0",
    "cross-env": "7.0.3",
    "dts-for-context-bridge": "0.7.1",
    "electron": "16.0.5",
    "electron-builder": "22.14.5",
    "electron-devtools-installer": "3.2.0",
    "electron-notarize": "^1.1.1",
    "eslint": "8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "nano-staged": "0.5.0",
    "playwright": "1.17.1",
    "postcss": "^8.4.5",
    "simple-git-hooks": "2.7.0",
    "tailwindcss": "^3.0.8",
    "typescript": "4.5.4",
    "vite": "2.7.7",
    "vite-plugin-svgr": "^0.6.0"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^0.19.3",
    "@codemirror/closebrackets": "^0.19.0",
    "@codemirror/commands": "^0.19.5",
    "@codemirror/comment": "^0.19.0",
    "@codemirror/history": "^0.19.0",
    "@codemirror/lang-javascript": "^0.19.2",
    "@codemirror/language": "^0.19.3",
    "@codemirror/matchbrackets": "^0.19.3",
    "@codemirror/state": "^0.19.2",
    "@codemirror/theme-one-dark": "^0.19.0",
    "@codemirror/view": "^0.19.9",
    "@dnd-kit/core": "^4.0.0",
    "@dnd-kit/sortable": "^5.0.0",
    "@fontsource/dm-sans": "^4.5.0",
    "@headlessui/react": "^1.4.0",
    "@typescript-eslint/parser": "^5.8.0",
    "chokidar": "^3.5.2",
    "dexie": "^3.0.3",
    "electron-store": "^8.0.1",
    "electron-updater": "4.6.5",
    "htmltojsx": "^0.3.0",
    "lodash": "^4.17.21",
    "query-string": "^7.0.1",
    "rc-tooltip": "^5.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hotkeys": "^2.0.0-pre9",
    "react-images-uploading": "^3.1.3",
    "react-inlinesvg": "^2.3.0",
    "react-multilevel-dropdown": "^3.0.3",
    "react-query": "^3.19.6",
    "react-router-dom": "^6.2.1",
    "svgo": "^2.8.0",
    "universal-analytics": "^0.4.23",
    "uuid": "^8.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Icon-Shelf/icon-shelf.git"
  },
  "author": {
    "name": "Robin",
    "url": "https://github.com/MrRobz"
  },
  "license": "MIT"
}
